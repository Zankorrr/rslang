{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "import/extensions": 0,


        "linebreak-style": ["error", "windows"],
				"eol-last": 0,
				"no-multiple-empty-lines": ["error", { "max": 1, "maxEOF": 0 }],
				"import/prefer--export": "off",
				"indent": "off",
				"no-console": "off",
				"no-param-reassign": 0,
				"allowUnreachableCode": true

    },
    "settings": {
        "import/resolver": {
            "node": {
                "extensions": [".js", ".jsx", ".ts", ".tsx"]
            }
        }
    }
}
